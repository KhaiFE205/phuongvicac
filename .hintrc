{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none",
          "user-select"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ]
  }
}